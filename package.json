{
  "name": "ts-workbench",
  "version": "0.0.1",
  "description": "TypeScript workbench",
  "author": "Jesse Chen",
  "keywords": [
    "typescript"
  ],
  "repository": "github:jesseminn/ts-workbench",
  "license": "ISC",
  "exports": {
    "./*": {
      "require": "./dist/*/index.js",
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    }
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "rm -rf ./dist && npx --yes tsc",
    "test": "jest",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2"
  }
}
